{
  "name": "hospital_appointment",
  "version": "1.0.0",
  "description": "Manage hospital appointments, track doctors, and organize medical visits",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "bash scripts/start-ios-clean.sh",
    "ios:quick": "expo run:ios",
    "web": "expo start --web --clear --port 8080",
    "dev": "npm run kill && npm run clean && expo start --clear",
    "dev:smart": "if xcrun simctl list devices | grep -q 'Booted'; then expo start --ios --clear; else expo start --web --clear --port 8080; fi",
    "dev:clean": "npm run kill && npm run clean && npm run dev:smart",
    "kill": "pkill -f 'expo|metro|node.*expo' || true",
    "clean": "rm -rf .expo && rm -rf node_modules/.cache && rm -rf /tmp/metro-* 2>/dev/null || true",
    "reset": "npm run kill && npm run clean && npm ci && expo start --web --clear --port 8080",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit 2>&1 | grep -v 'error TS2607\\|error TS2786' || true",
    "prepare": "husky",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace hospital_appointment.xcworkspace -scheme hospital_appointment -configuration Release archive",
    "security:scan": "osv-scanner --lockfile package-lock.json || echo '⚠️  Install osv-scanner: go install github.com/google/osv-scanner/cmd/osv-scanner@v1'",
    "security:scan:all": "osv-scanner --recursive . || echo '⚠️  Install osv-scanner: go install github.com/google/osv-scanner/cmd/osv-scanner@v1'",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "security:check": "npm run security:scan && npm run security:audit",
    "i18n:setup": "node node_modules/@umituz/react-native-localization/scripts/setup-languages.js",
    "i18n:translate": "node node_modules/@umituz/react-native-localization/scripts/translate-missing.js",
    "i18n:check": "node node_modules/@umituz/react-native-localization/scripts/check-translations.js",
    "i18n:analyze": "node node_modules/@umituz/react-native-localization/scripts/analyze-keys.js",
    "i18n:remove-unused": "node node_modules/@umituz/react-native-localization/scripts/remove-unused-keys.js",
    "localization:create-loaders": "node node_modules/@umituz/react-native-localization/scripts/createLocaleLoaders.js"
  },
